{
  "name": "backend",
  "version": "0.0.0",
  "description": "backend",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start": "pm2-runtime start ./dist/index.js",
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "test": "jest",
    "test:unit": "jest '.spec.ts'",
    "test:watch": "jest --watch",
    "docker:mysql": "docker run  -p 127.0.0.1:3306:3306 --restart unless-stopped --name birdie-mysql -e MYSQL_ROOT_PASSWORD=root -d mysql:5.7",
    "dev": "nodemon"
  },
  "dependencies": {
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "app-root-path": "^3.0.0",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "helmet": "^4.6.0",
    "knex": "^0.95.14",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "pino": "^7.4.0",
    "pino-http": "^6.0.0",
    "pino-pretty": "^7.2.0",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraff": "0.0.1-security",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.177",
    "@types/node": "^15.0.2",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "preset": "ts-jest"
